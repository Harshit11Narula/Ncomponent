{
  "name": "nitrozen",
  "version": "0.1.0",
  "private": false,
  "main": "dist/nitrozen.umd.js",
  "module": "dist/nitrozen.umd.js",
  "unpkg": "dist/nitrozen.umd.min.js",
  "scripts": {
